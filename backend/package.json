{
  "name": "@quiz/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build && concurrently -k -n TSC,NODE \"tsc -w -p tsconfig.json\" \"node --watch --enable-source-maps dist/src/index.js\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "seed": "npm run build && node dist/scripts/seed.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "typescript": "^5.5.0"
  }
}
